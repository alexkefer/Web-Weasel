{
  "manifest_version": 3,
  "name": "P2P Web Cache",
  "version": "1.0",
  "description": "A Chrome extension facilitating peer-to-peer file sharing",
  "permissions": [
    "activeTab",
    "http://localhost:8080/*"
  ],
  "host_permissions": [
    "activeTab",
    "http://localhost:8080/*"
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "P2P Web Cache Browser Extension",
    "default_popup": "src/pages/popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
